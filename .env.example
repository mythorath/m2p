# M2P (Mine to Play) Environment Variables

# Database
DATABASE_URL=sqlite:///./m2p.db

# Flask
FLASK_ENV=development
SECRET_KEY=your-secret-key-change-in-production

# Server
HOST=0.0.0.0
PORT=5000

# Frontend
REACT_APP_SERVER_URL=http://localhost:5000

# Pool Polling
POOL_POLLING_INTERVAL=60

# Logging
LOG_LEVEL=INFO
